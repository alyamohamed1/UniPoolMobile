{
  "expo": {
    "name": "Unipool",
    "slug": "unipool-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#7F7CAF",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unipool.mobile",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show nearby rides"
      }
    },
    "android": {
      "package": "com.unipool.mobile",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    }
  }
}